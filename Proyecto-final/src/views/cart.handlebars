<div>
  <table>
    <tr>
    <th>Carrito</th>
    <th>Id del Producto</th>
    <th>Cantidad</th>
    
  </tr>
{{#each payload}}
        <tr class="product-row" data-product-id="{{this.product}}">
          <td>{{../cartId}}</td>
            <td>{{this.product}}</td> 
          <td>{{this.quantity}}</td>
          <td>   <a href="#" class="deleteButton" data-cid='{{../cartId}}' data-pid='{{this.product}}'><svg xmlns="http://www.w3.org/2000/svg" width="29" height="29" viewBox="2 2 20 20" fill="none" stroke="#d87093" stroke-width="1" stroke-linecap="round" stroke-linejoin="round"><polyline points="3 6 5 6 21 6"></polyline><path d="M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2"></path><line x1="10" y1="11" x2="10" y2="17"></line><line x1="14" y1="11" x2="14" y2="17"></line></svg></a>
      </td>
         
        </tr>


    
    {{/each }}
  </table>  
  <div id='btns'>
           <button type="submit" id="emptyButton" data-cid='{{cartId}}'> Vaciar Carrito</button>
  </div>
  <form action="/carts/{{cartId}}/purchase" method="POST">
    <button type="submit">Finalizar Compra</button>
  </form>
 
<a href="/products" role="button">Volver a los productos</a>
</div><script src="/socket.io/socket.io.js"></script>
<script src="/js/cart.js">

</script>